<h2>Show site</h2>

<ul>

  <li>
    <strong>Name:</strong>
    <%= @site.name %>
  </li>

  <li>
    <strong>Latitude:</strong>
    <%= @site.latitude %>
  </li>

  <li>
    <strong>Longitude:</strong>
    <%= @site.longitude %>
  </li>

</ul>

<%= link "Edit", to: site_path(@conn, :edit, @site) %>
<%= link "Back", to: site_path(@conn, :index) %>
